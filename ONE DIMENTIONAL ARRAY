#include<stdio.h>
int main() {
    int n;

    printf("Enter the number of elements: ");
    scanf("%d", &n);

    int array[n]; 

    printf("Enter %d numbers:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &array[i]);
    }

    printf("You entered: ");
    for(int i = 0; i < n; i++) {
        printf("%d ", array[i]);
    }
    
    return 0;
}
