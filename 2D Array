#include <stdio.h>

int main() 
{
    int matrix[2][5];
    
  
    printf("Enter 10 values for a 2x5 matrix:\n");
    for (int i = 0; i < 2; i++) {         
        for (int j = 0; j < 5; j++) {     
            printf("Enter value for matrix[%d][%d]: ", i, j);
            scanf("%d", &matrix[i][j]);    // Take input from userthe 
        }
    }
    
    // Print the matrix
    printf("\nThe matrix you entered is:\n");
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 5; j++) {
            printf("%d ", matrix[i][j]);   // Print each element
        }
        printf("\n");  // Move to the next line after each row
    }

    return 0;
}
