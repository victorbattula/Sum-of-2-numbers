#include <stdio.h>
int main() {
    char str[100];
    char search;
    int count = 0;
    printf("Enter a string: ");
    scanf("%s", str);
    printf("Enter character to search: ");
    scanf(" %c", &search);
    for (int i = 0; str[i] != '\0'; i++) {
        if (str[i] == search) {
            count++;
        }
    }
    printf("Frequency of '%c': %d\n", search, count);
    return 0;
}
