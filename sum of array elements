#include <stdio.h>

int main() {
    int array[5];
    int sum = 0;

    printf("Enter 5 integers: ");

    // Input array elements
    for (int i = 0; i < 5; i++) {
        scanf("%d", &array[i]);
    }

    // Calculate sum
    for (int i = 0; i < 5; i++) {
        sum += array[i];
    }

    printf("Sum of array elements: %d\n", sum);

    return 0;
}
