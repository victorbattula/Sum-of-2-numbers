#include <stdio.h>
#include <string.h>
int main() {
    char str[2][2][20];
    strcpy(str[0][0], "VICTOR");
    strcpy(str[0][1], "BATTULA");
    strcpy(str[1][0], "C");
    strcpy(str[1][1], "Programming");
    printf("%s %s\n", str[0][0], str[0][1]);
    printf("%s %s\n", str[1][0], str[1][1]);
    return 0;
}
